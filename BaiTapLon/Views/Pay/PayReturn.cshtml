@{
    ViewBag.Title = "PayReturn";
}
@model  string
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>RETURN URL FROM VNPAY</title>
    <link href="Styles/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <div class="header clearfix">

            <h3 class="text-muted">Kết quả thanh toán</h3>
        </div>
        <div class="table-responsive">
            <div runat="server" id="displayMsg">@ViewBag.displayMsg</div>
        </div>
        <div class="table-responsive">
            <div runat="server" id="displayTmnCode">@ViewBag.displayTmnCode</div>
        </div>
        <div class="table-responsive">
            <div runat="server" id="displayTxnRef">@ViewBag.displayTxnRef</div>
        </div>
        <div class="table-responsive">
            <div runat="server" id="displayVnpayTranNo">@ViewBag.displayVnpayTranNo</div>
        </div>
        <div class="table-responsive">
            <div runat="server" id="displayAmount">@ViewBag.displayAmount</div>
        </div>
        <div class="table-responsive">
            <div runat="server" id="displayBankCode">@ViewBag.displayBankCode</div>
        </div>
    </div>
